doctype html
html 
    head 
        title= Home
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Object Table
        a.update-button(href="/update-cobj") Add/Update this table
        .cards 
            table
                thead
                    tr
                        th First Name
                        th Last Name 
                        th Email
                tbody
                    each contact in data
                        tr
                            td #{contact.properties.firstname}
                            td #{contact.properties.lastname}
                            td #{contact.properties.email}
        